<?xml version="1.0"?>
<!-- 
Umbraco examine is an extensible indexer and search engine.
This configuration file can be extended to create your own index sets.
Index/Search providers can be defined in the UmbracoSettings.config

More information and documentation can be found on GitHub: https://github.com/Shazwazza/Examine/
-->
<ExamineLuceneIndexSets>
  <!-- The internal index set used by Umbraco back-office - DO NOT REMOVE -->
  <IndexSet SetName="InternalIndexSet" IndexPath="~/App_Data/TEMP/ExamineIndexes/Internal/"/>

  <!-- The internal index set used by Umbraco back-office for indexing members - DO NOT REMOVE -->
  <IndexSet SetName="InternalMemberIndexSet" IndexPath="~/App_Data/TEMP/ExamineIndexes/InternalMember/">
    <IndexAttributeFields>
      <add Name="id" />
      <add Name="nodeName"/>
      <add Name="updateDate" />
      <add Name="writerName" />
      <add Name="loginName" />
      <add Name="email" />
      <add Name="nodeTypeAlias" />
    </IndexAttributeFields>
  </IndexSet>

  <!-- Default Indexset for external searches, this indexes all fields on all types of nodes-->
  <IndexSet SetName="ExternalIndexSet" IndexPath="~/App_Data/TEMP/ExamineIndexes/External/" />

  <IndexSet SetName="ElasticIndexSet"  IndexPath="~/App_Data/TEMP/ExamineIndexes/Elastic/" >
    <IndexAttributeFields>
      <add Name="id" />
      <add Name="nodeName"/>
      <add Name="updateDate" />
      <add Name="writerName" />
      <add Name="loginName" />
      <add Name="email" />
      <add Name="nodeTypeAlias" />
      <add Name="urlName" />
    </IndexAttributeFields>
    <IndexUserFields>
      <add Name="preamble" />
      <add Name="mainContent" />
      <add Name="resourcesItems" />
      <add Name="richText" />
      <add Name="biographyContent" />
      <add Name="directoryListingContent" />
      <add Name="projectsContent" />
      <add Name="publicationsContent" />
      <add Name="researchContent" />
    </IndexUserFields>
    <ExcludeNodeTypes>
      <add Name="Image" />
      <add Name="Folder" />
      <add Name="toBeDeleted" />
    </ExcludeNodeTypes>
  </IndexSet>
</ExamineLuceneIndexSets>
